{
  "name": "babitz",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --spec 'cypress/integration/*.spec.js'",
    "cypress:coverage": "nyc npm run cypress:run",
    "coverage:report": "nyc report --check-coverage --statements 50",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prettier": "prettier '**pages/*.{js,jsx,vue}' '**pages/api/*.{js,jsx,vue}' --ignore-unknown",
    "prettier:check": "npm run prettier -l",
    "prettier:fix": "npm run prettier --write"
  },
  "dependencies": {
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "vercel": "^23.0.1"
  },
  "devDependencies": {
    "eslint": "7.30.0",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^26.4.2",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2"
  }
}
